<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="AnimationTableSchema"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/AnimationTableSchema.xsd"
    xmlns:mstns="http://tempuri.org/AnimationTableSchema.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:types="http://tempuri.org/ReusableTypes.xsd"
>
  <xs:import namespace="http://tempuri.org/ReusableTypes.xsd" schemaLocation="ReusableTypes.xsd"/>
  <xs:element name ="MarioAnimations">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Animation" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:attribute name="value" use="required" type="xs:int"/>
            <xs:attribute name="name" use="required" type="xs:string"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>